<odoo>
    <data>
        <record model="ir.ui.view" id="university_program_form_view">
            <field name="name">university_program_form_view</field>
            <field name="model">university.program</field>
            <field name="arch" type="xml">
                <form>
                    <header>
<!--                        <button type="object" string="Test Token"/>-->
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="coordinator"/>
                            </group>
                        </group>
                        <notebook>
                            <page name="documents" string="Documents">
                                <field name="documents" >
                                    <tree>
                                        <field name="name"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record model="ir.ui.view" id="university_program_tree_view">
            <field name="name">university_program_tree_view</field>
            <field name="model">university.program</field>
            <field name="priority" eval="60"/>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="coordinator"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>