<odoo>
    <data>

        <record id='hl_base_university_program_cost_action_window' model='ir.actions.act_window'>
			<field name="name">Services</field>
			<field name="res_model">university.program.cost</field>
			<field name="view_mode">tree,form</field>
			<field name="domain">[]</field>
			<field name="context">{}</field>
			<field name="limit">20</field>
		</record>

        <record id="hl_base_university_program_cost_tree_action_window_view" model="ir.actions.act_window.view">
			<field name="act_window_id" ref="hl_base_university_program_cost_action_window" />
			<field name="view_id" ref="hl_base_university_program_cost_tree_view" />
			<field name="view_mode">tree</field>
			<field name="sequence" eval="1"/>
		</record>

		<record id="hl_base_university_program_cost_form_action_window_view" model="ir.actions.act_window.view">
			<field name="act_window_id" ref="hl_base_university_program_cost_action_window" />
			<field name="view_id" ref="hl_base_university_program_cost_form_view" />
			<field name="view_mode">form</field>
			<field name="sequence" eval="3"/>
		</record>

    </data>
</odoo>