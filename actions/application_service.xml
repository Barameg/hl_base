<odoo>
    <data>

        <record id='hl_base_application_service_action_window' model='ir.actions.act_window'>
			<field name="name">Services</field>
			<field name="res_model">application.service</field>
			<field name="view_mode">tree,form</field>
			<field name="domain">[]</field>
			<field name="context">{}</field>
			<field name="limit">20</field>
		</record>

        <record id="hl_base_application_service_tree_action_window_view" model="ir.actions.act_window.view">
			<field name="act_window_id" ref="hl_base_application_service_action_window" />
			<field name="view_id" ref="hl_base_application_service_tree_view" />
			<field name="view_mode">tree</field>
			<field name="sequence" eval="1"/>
		</record>

		<record id="hl_base_application_service_form_action_window_view" model="ir.actions.act_window.view">
			<field name="act_window_id" ref="hl_base_application_service_action_window" />
			<field name="view_id" ref="hl_base_application_service_form_view" />
			<field name="view_mode">form</field>
			<field name="sequence" eval="3"/>
		</record>

    </data>
</odoo>